<h2>Create Student</h2>
<%= form_for :student, url: students_path do |f| %>
  
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.submit "Create Student", :class => "btn btn-primary btn-sm" %>
  </p>
<% end %>
 
<%= link_to 'Back', students_path, :class => "btn btn-info btn-sm" %>